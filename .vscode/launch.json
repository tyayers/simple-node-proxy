      "program": "${workspaceFolder}/src/index.js",
      "env": {
        "PORT": "8081",
        "TARGET": "http://localhost:8080",
        "GOOGLE_APPLICATION_CREDENTIALS": "svc-key.json"
      }
    }
  ]
}